                                <div class="row">
                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label asp-for="GSTDetails.Address" class="col-sm-4 control-label"></label>
                                            <div class="col-sm-8">
                                                <input id="txtGstAddress" type="text" asp-for="GSTDetails.Address" class="form-control" />
