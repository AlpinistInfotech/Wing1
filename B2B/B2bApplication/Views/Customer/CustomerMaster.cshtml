                                        <div class="form-group">
                                            <label asp-for="GSTDetails.Email" class="col-sm-4 control-label"></label>
                                            <div class="col-sm-8">
                                                <input id="txtGSTEmail" type="email" asp-for="GSTDetails.Email" class="form-control" />
                                                <span asp-validation-for="GSTDetails.Email" class="text-danger"></span>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-6">
