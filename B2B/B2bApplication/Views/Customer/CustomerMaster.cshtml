                                        <label asp-for="customerMaster.StateId" class="col-sm-4 control-label"></label>
                                        <div class="col-sm-8">
                                            <select id="ddl_state_id" asp-for="customerMaster.StateId" class="form-control select2" asp-items="@ViewBag.StateList">
                                            </select>
                                            <span asp-validation-for="customerMaster.StateId" class="text-danger"></span>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="row">
                                <div class="col-md-6">
                                    <div class="form-group">
                                        <label asp-for="customerMaster.PinCode" class="col-sm-4 control-label"></label>
                                        <div class="col-sm-8">
                                            <input id="txtPinCode" type="text" asp-for="customerMaster.PinCode" class="form-control" />
                                            <span asp-validation-for="customerMaster.PinCode" class="text-danger"></span>
                                        </div>
                                    </div>
                                </div>

                                <div class="col-md-6">
                                    <div class="form-group">
                                        <label asp-for="customerMaster.CustomerType" class="col-sm-4 control-label"></label>
                                        <div class="col-sm-8">
                                            <select asp-for="customerMaster.CustomerType" class="form-control select2">

                                                @if (Model.customerMaster.CustomerType == enmCustomerType.Admin)
                                                {
                                                    <option value="@((int)enmCustomerType.Admin)">@enmCustomerType.Admin</option>
                                                }
                                                else
                                                {
                                                    <option value="@((int)enmCustomerType.B2B)">@enmCustomerType.B2B</option>
                                                    <option value="@((int)enmCustomerType.B2C)">@enmCustomerType.B2C</option>
                                                    <option value="@((int)enmCustomerType.MLM)">@enmCustomerType.MLM</option>
                                                    <option value="@((int)enmCustomerType.InHouse)">@enmCustomerType.InHouse</option>
                                                }

                                            </select>
                                            <span asp-validation-for="customerMaster.CustomerType" class="text-danger"></span>
                                        </div>
                                    </div>
                                </div>


                            </div>


                            <div class="row">
                                <div class="col-md-6">
                                    <div class="form-group">
                                        <input type="hidden" asp-for="customerMaster.Logo" />

                                        <label asp-for="Logo" class="col-sm-4 control-label"></label>
                                        <div class="col-sm-8">
                                            <div class="custom-file">
                                                <input id="logofile" rel="icon" type="file" asp-for="Logo" class="custom-file-input" accept="image/x-icon" />
                                                
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="col-md-6">
                                        <div class="form-group">
                                            @if (Model.LogoData != null)
                                            {
                                                <div class="form-group col-md-4">
                                                    <img width="100px" height="100px" src="@string.Format("data:image/ico;base64,{0}", Convert.ToBase64String(Model.LogoData))" />
                                                </div>
                                            }
                                        </div>
                                    </div>
                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label asp-for="customerMaster.IsActive" class="col-sm-8 control-label"></label>
                                            <div class="col-sm-4">
                                                <label class="input-group-addon input-group-addon-custom">
                                                    <input id="chkIsActive" class="ace ace-switch" type="checkbox" asp-for="customerMaster.IsActive" />
                                                    <span class="lbl" data-lbl="Yes&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;no"></span>
                                                </label>
                                                <span asp-validation-for="customerMaster.IsActive" class="text-danger"></span>
                                            </div>
                                        </div>
                                    </div>

                                </div>
                            </div>

                        }

                        @if (Model.pan != null)
                        {
                            <div class="space"></div>
                            <h4 class="header blue bolder smaller">Pan Details</h4>
                            <div class="row">
                                <div class="col-md-6">
                                    <div class="form-group">
                                        <label asp-for="pan.PANNo" class="col-sm-4 control-label"></label>
                                        <div class="col-sm-8">
                                            <input type="text" asp-for="pan.PANNo" class="form-control" />
                                            <span asp-validation-for="pan.PANNo" class="text-danger"></span>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="form-group">
                                        <label asp-for="pan.PANName" class="col-sm-4 control-label"></label>
                                        <div class="col-sm-8">
                                            <input type="text" asp-for="pan.PANName" class="form-control" />
                                            <span asp-validation-for="pan.PANName" class="text-danger"></span>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        }

                        @if (Model.GSTDetails != null)
