                                        </div>
                                    </div>
                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label asp-for="GSTDetails.CountryId" class="col-sm-4 control-label"></label>
                                            <div class="col-sm-8">
                                                <select id="ddl_gst_country_id" asp-for="GSTDetails.CountryId" class="form-control select2" asp-items="@ViewBag.CountryList">
                                                </select>
                                                <span asp-validation-for="GSTDetails.CountryId" class="text-danger"></span>
                                            </div>
                                        </div>
                                    </div>

                                </div>
                                <div class="row">
                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label asp-for="GSTDetails.StateId" class="col-sm-4 control-label"></label>
                                            <div class="col-sm-8">
                                                <select id="ddl_gst_state_id" asp-for="GSTDetails.StateId" class="form-control select2" asp-items="@ViewBag.GStStateList">
                                                </select>
                                                <span asp-validation-for="GSTDetails.StateId" class="text-danger"></span>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label asp-for="GSTDetails.PinCode" class="col-sm-4 control-label"></label>
                                            <div class="col-sm-8">
                                                <input id="txtGstPincode" type="text" asp-for="GSTDetails.PinCode" class="form-control" />
                                                <span asp-validation-for="GSTDetails.PinCode" class="text-danger"></span>
                                            </div>
                                        </div>
                                    </div>

                                </div>

                            </div>
                        }


                        @if (Model.banks != null)
                        {
                            <div class="space"></div>
                            <h4 class="header blue bolder smaller">Bank Details</h4>


                            <div class="row">
                                <div class="col-md-6">
                                    <div class="form-group">
                                        <label asp-for="banks.BankId" class="col-sm-4 control-label"></label>
                                        <div class="col-sm-8">
