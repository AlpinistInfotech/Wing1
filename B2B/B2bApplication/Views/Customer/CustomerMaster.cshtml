                            <input type="hidden" asp-for="customerSetting.MPin" class="form-control" />
                            <div class="form-group">
                                <label class="col-sm-3 control-label no-padding-right" for="form-field-pass1">Mpin</label>
                                <div class="col-sm-9">
                                    <input type="password" asp-for="NewMpin" class="form-control" />
                                    <span asp-validation-for="NewMpin" class="text-danger"></span>
                                </div>
                            </div>

                            <div class="space-4"></div>

                            <div class="form-group">
                                <label class="col-sm-3 control-label no-padding-right" for="form-field-pass2">Confirm MPin</label>

                                <div class="col-sm-9">
                                    <input type="password" asp-for="ConfirmNewMpin" class="form-control" />
                                    <span asp-validation-for="ConfirmNewMpin" class="text-danger"></span>
                                </div>
                            </div>
                        </div>
                    }
                </div>
            </div>

            <div class="clearfix form-actions">
                <div class="col-md-offset-3 col-md-9">
                    <button class="btn btn-info" type="submit">
                        <i class="ace-icon fa fa-check bigger-110"></i>
                        Save
                    </button>

                    &nbsp; &nbsp;
                    <a class="btn" href="/Customer/CustomerMaster">
                        <i class="ace-icon fa fa-undo bigger-110"></i>
                        Reset
                    </a>

                </div>
            </div>
        </form>
    </div><!-- /.span -->
</div><!-- /.user-profile -->

@section Scripts {
    <script type="text/javascript">
        jQuery(function ($) {
            $('.select2').select2();   
            var file = $('#logofile')//form.find('#logofile').eq(0);
            file.ace_file_input();
            //file.ace_file_input({
            //    style: 'well',
            //    btn_choose: 'Click to choose Logo',
            //    btn_change: null,
            //    no_icon: 'ace-icon fa fa-picture-o',
            //    thumbnail: 'small',
            //    before_remove: function () {
            //        //don't remove/reset files while being uploaded
            //        return !working;
            //    },
            //    //allowExt: ['jpg', 'jpeg', 'png', 'gif'],
            //    //allowMime: ['image/jpg', 'image/jpeg', 'image/png', 'image/gif']
            //    allowExt: ['ico'],
            //    allowMime: ['image/x-ico']
            //});


        });
        $(document).ready(function () {
            $(window).load(function () {
                if ($("#cboxHaveGST").prop("checked") == true) {
                    $('#divGstDetails').show();
                    $('#divGstDetails').prop("disabled", false);
                } else {
                    $('#divGstDetails').hide();                    
                    $('#divGstDetails').prop("disabled", true);
                }
            });
            $("#ddl_country_id").change(function () {
                LoadState($("#ddl_country_id").val(),"ddl_state_id")
            });
            $("#ddl_gst_country_id").change(function () {
                LoadState($("#ddl_gst_country_id").val(), "ddl_gst_state_id")
            });

            $("#cboxHaveGST").change(function () {
                if (this.checked) {
                    $('#divGstDetails').show();
                    $('#divGstDetails').prop("disabled", false);
                } else {
                    $('#divGstDetails').hide();
                    $('#divGstDetails').prop("disabled", true);
                }
            });

            $("#txtCustomerName").blur(function () {
                if ($("#txtRegisteredName").val() == "" || $("#txtRegisteredName").val() == null) {
                    $("#txtRegisteredName").val($("#txtCustomerName").val());
                }
            });
            $("#txtCustomermail").blur(function () {
                if ($("#txtGSTEmail").val() == "" || $("#txtGSTEmail").val() == null) {
                    $("#txtGSTEmail").val($("#txtCustomermail").val());
                }
            });
            $("#txtCustomerContact").blur(function () {
                if ($("#txtGSTMobile").val() == "" || $("#txtGSTMobile").val() == null) {
                    $("#txtGSTMobile").val($("#txtCustomerContact").val());
                }
            });
            $("#txtPinCode").blur(function () {
                if ($("#txtGstPincode").val() == "" || $("#txtGstPincode").val() == null) {
                    $("#txtGstPincode").val($("#txtPinCode").val());
                }
            });

            $("#ddl_state_id").change(function () {
                if ($("#ddl_gst_state_id").val() == "" || $("#ddl_gst_state_id").val() == null) {
                    $("#ddl_gst_state_id").val($("#ddl_state_id").val());
                }
            });


            
            function removeOptions(selectElement) {
                var i, L = selectElement.options.length - 1;
                for (i = L; i >= 0; i--) {
                    selectElement.remove(i);
                }
            }

            function LoadState(countryId, controlName ) {

                //let countryId = parseInt(document.getElementById(controlName).value);

                let requestUrl = "/Customer/GetState/" + countryId ;
                $.ajax({ url: requestUrl }).done(function (data) {

                     var statedropdown = document.getElementById(controlName)
                    if (statedropdown != null) {
                        
                        removeOptions(statedropdown);
                        var defaultstoption = document.createElement("option");
                        defaultstoption.text = "Select State";
                        statedropdown.add(defaultstoption);

                        
                        $.each(data, function (index, item) {
                            var option = document.createElement("option");
                            option.text = item.stateName;
                            option.value = item.stateId;
                            statedropdown.add(option);                            
                        });
                    }
                }).fail(function (jqXHR, exception) {
                    // Our error logic here
                    var msg = '';
                    if (jqXHR.status === 0) {
                        msg = 'Not connect.\n Verify Network.';
                    } else if (jqXHR.status == 400) {
                        msg = 'Invalid Country';
                    } 
                    else if (jqXHR.status == 404) {
                        msg = 'Requested page not found. [404]';
                    } else if (jqXHR.status == 500) {
                        msg = 'Internal Server Error [500].';
                    } else if (exception === 'parsererror') {
                        msg = 'Requested JSON parse failed.';
                    } else if (exception === 'timeout') {
                        msg = 'Time out error.';
                    } else if (exception === 'abort') {
                        msg = 'Ajax request aborted.';
                    } else {
                        msg = 'Uncaught Error.\n' + jqXHR.responseText;
                    }
                    alert(msg);
                })

                
            }
        });






