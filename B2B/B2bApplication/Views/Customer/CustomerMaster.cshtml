            $(window).load(function () {
                if ($("#cboxIsAllApplicable").prop("checked") == true) {
                    $('#txtIPAddess').hide();
                } else {
                    $('#txtIPAddess').show();
                }
            })
            $("#cboxIsAllApplicable").change(function () {
                if (this.checked) {
                    $('#txtIPAddess').hide();
                } else {
                    $('#txtIPAddess').show();
                }
            });


        });



    </script>
