@model List<mdlWingMarkupReport>

<div class="row">
    <div class="col-xs-12">        
        <div class="clearfix">
            <div class="pull-right tableTools-container"></div>
        </div>
        <div class="table-header">
            Results for "All Markup"
        </div>

        <!-- div.table-responsive -->
        <!-- div.dataTables_borderWrap -->
        <div>
            <table id="dynamic-table" class="table table-striped table-bordered table-hover">
                <thead>
                    <tr>
                        <th>
                            Edit/Delete
                        </th>
                        <th>Applicability</th>
                        <th>Flight Type</th>
                        <th>Service Provider</th>
                        <th>Customer Type</th>
                        <th>Customer</th>
                        <th>Passenger</th>
                        <th>Flight Class</th>
                        <th>Airline</th>
                        <th>Amount</th>
                        <th>From Date</th>
                        <th>To Date</th>
                    </tr>
                </thead>

                <tbody>
                    @foreach (var md in Model)
                    {
                    <tr>
                        <td class="center">
                            <div class="hidden-sm hidden-xs action-buttons">
                                <a class="blue" href="#">
                                    <i class="ace-icon fa fa-search-plus bigger-130"></i>
                                </a>
                                <a class="green" href="/Home/WingMarkup/@md.Id">
                                    <i class="ace-icon fa fa-pencil bigger-130"></i>
                                </a>
                                <a class="red" href="/Home/WingMarkup/@md.Id">
                                    <i class="ace-icon fa fa-trash-o bigger-130"></i>
                                </a>
                            </div>

                            <div class="hidden-md hidden-lg">
                                <div class="inline pos-rel">
                                    <button class="btn btn-minier btn-yellow dropdown-toggle" data-toggle="dropdown" data-position="auto">
                                        <i class="ace-icon fa fa-caret-down icon-only bigger-120"></i>
                                    </button>

                                    <ul class="dropdown-menu dropdown-only-icon dropdown-yellow dropdown-menu-right dropdown-caret dropdown-close">
                                        <li>
                                            <a href="#" class="tooltip-info" data-rel="tooltip" title="View">
                                                <span class="blue">
                                                    <i class="ace-icon fa fa-search-plus bigger-120"></i>
                                                </span>
                                            </a>
                                        </li>

                                        <li>
                                            <a href="/Home/WingMarkup/@md.Id" class="tooltip-success" data-rel="tooltip" title="Edit">
                                                <span class="green">
                                                    <i class="ace-icon fa fa-pencil-square-o bigger-120"></i>
                                                </span>
                                            </a>
                                        </li>

                                        <li>
                                            <a href="/Home/WingMarkup/@md.Id" class="tooltip-error" data-rel="tooltip" title="Delete">
                                                <span class="red">
                                                    <i class="ace-icon fa fa-trash-o bigger-120"></i>
                                                </span>
                                            </a>
                                        </li>
                                    </ul>
                                </div>
                            </div>
                        </td>
                        <td>
                            @md.FlightType
                        </td>
                        <td>
                            @md.Applicability
                        </td>
                        <td>
                            @md.ServiceProvider
                        </td>
                        <td>
                            @md.CustomerType
                        </td>
                        <td>
                            @md.Customer
                        </td>
                        <td>
                            @md.PassengerType
                        </td>
                        <td>
                            @md.FlightClass
                        </td>
                        <td>
                            @md.Airline
                        </td>
                        <td>
                            @md.Amount
                        </td>
                        <td>
                            @md.EffectiveFromDt.ToString("dd-MMM-yyyy")
                        </td>
                        <td>
                            @md.EffectiveToDt.ToString("dd-MMM-yyyy")
                        </td>

                    </tr>
                        }
                </tbody>
            </table>
        </div>
    </div>
</div>